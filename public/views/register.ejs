
                <div class="col-lg-12">
                    <form ng-submit="register(user.email, user.password,user.passwordConfirm)">

                        <div class="form-group">
                            <input type="text"
                                   class="form-control"
                                   placeholder="Email"
                                   ng-model="user.email">
                        </div>
                        <div class="form-group">
                            <input type="password"
                                   class="form-control"
                                   placeholder="Password"
                                   ng-model="user.password">
                        </div>
                        <div class="form-group">
                            <input type="password Confirm"
                                   class="form-control"
                                   placeholder="Password"
                                   ng-model="user.passwordConfirm">
                        </div>
                        <div class="form-group">

                                <button type="submit" class="btn btn-default">Register</button>

                        </div>
                    </form>
                </div>
            </div>
