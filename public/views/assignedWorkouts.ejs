
    <div class="container-fluid">
        <div class="col-sm-12">
            <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6 col-xl-2"
                 dir-paginate="ex in workouts | filter:q | itemsPerPage: pageSize" current-page="currentPage"
                 style="padding: 10px">
                <div class="panel panel-success" style="margin:0;padding:0;border: 0">

                    <div class="panel-body" style="padding: 0;background-color: #16a085;color: white">
                        <div class="col-sm-12" style="padding: 0">

                            <p style="text-align: left;font-size: 14px;margin-top: 5px;margin-bottom: 0px;text-align: center;font-weight: bolder">
                                {{ex.workoutName}}</p>

                        </div>

                    </div>

                </div>
                <div class="col-lg-12" style="padding: 0">
                    <div class="exercise-label"><p class="pull-left">Start Time:</p> {{ex.start_date | date: 'dd, MMMM
                        HH:mm'}}
                    </div>
                </div>
                <div class="col-lg-12" style="padding: 0">
                    <div class="exercise-label"><p class="pull-left">End Time:</p>{{ex.end_date | date: 'dd, MMMM
                        HH:mm'}}
                    </div>
                </div>


            </div>
            <div class="col-sm-12">
                <div ng-controller="OtherController" class="other-controller">
                    <div class="text-center">
                        <dir-pagination-controls boundary-links="true"
                                                 on-page-change="pageChangeHandler(newPageNumber)"
                                                 template-url="/views/dirPagination.tpl.ejs"></dir-pagination-controls>
                    </div>
                </div>
            </div>
        </div>
    </div>
