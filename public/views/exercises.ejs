<div class="col-sm-10 col-md-10 col-lg-10" >
    <div class="container-fluid">
        <div class="col-sm-12">
            <div class="col-xs-4">
                <div class="form-group">
                    <div class="input-group input-group-rounded">
                  <span class="input-group-btn">
                    <button class="btn" type="submit"><span class="fui-search"></span></button>
                  </span>
                        <input ng-model="q" id="search" class="form-control" placeholder="Filter text">
                    </div>
                </div>
            </div>
            <div class="col-xs-4">


            </div>
            <div class="col-xs-4">
                <div class="form-group">
                    <label for="search">items per page:</label>
                    <input type="checkbox" checked data-toggle="switch" name="custom-switch-text" data-on-text="12" data-off-text="28" id="switch-10" />
                </div>
            </div>
        </div>
        <br>
        <div class="col-sm-4" dir-paginate="ex in exercises | filter:q | itemsPerPage: pageSize" current-page="currentPage">
            <div class="panel panel-primary">
                <div class="panel-body">
                {{ex.name}}
                </div>
            </div>
        </div>

        <div class="col-sm-12">
            <div ng-controller="OtherController" class="other-controller">
                <div class="text-center">
                    <dir-pagination-controls boundary-links="true" on-page-change="pageChangeHandler(newPageNumber)" template-url="/views/dirPagination.tpl.ejs"></dir-pagination-controls>
                </div>
            </div>
        </div>


    </div>

