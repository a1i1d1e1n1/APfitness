<div class="col-sm-9 col-md-9 col-lg-9" style="padding-left: 0">
    <div class="container-fluid">
        <div class="col-sm-12">
            <div class="col-xs-4">
                <div class="form-group">
                    <div class="input-group input-group-rounded">
                  <span class="input-group-btn">
                    <button class="btn" type="submit"><span class="fui-search"></span></button>
                  </span>
                        <input ng-model="q" id="search" class="form-control" placeholder="Search">
                    </div>
                </div>
            </div>
        </div>
        <div class="col-sm-12">

            <div class="col-sm-6 col-md-4 col-lg-3 col-xl-2" ng-init="p = this"
                 dir-paginate="workout in workouts | filter:q | itemsPerPage: pageSize" current-page="currentPage" style="padding: 10px;">
                <div class="panel panel-success" style="margin:0;padding:0;border: 0">

                    <div class="panel-body" style="padding: 0;background-color: #16a085;color: white">
                        <div class="col-sm-12" style="padding: 0">

                            <p style="text-align: left;font-size: 14px;margin-top: 5px;margin-bottom: 0px;text-align: center;font-weight: bolder">
                                {{workout.name}}</p>

                        </div>
                        <div class="col-sm-12" style="padding: 0">
                            <div ng-repeat="exercises in workouts_exercises">

                                    {{exercises}}


                            </div>
                        </div>


                    </div>

                </div>
                <div class="col-lg-6" style="padding: 0">
                    <div class="exercise-label">Rating: 0</div>
                </div>
                <div class="col-lg-6" style="padding: 0">
                    <div class="exercise-label">Comments: 0</div>
                </div>
                <div class="col-lg-12" style=";padding: 0">
                    <button class="btn btn-default btn-sm" style="width: 100%;border-top-left-radius: 0;border-top-right-radius: 0"><i class="fui-info-circle"></i> </button>
                </div>

            </div>
        </div>
        <div class="col-sm-12">
            <div ng-controller="OtherController" class="other-controller">
                <div class="text-center">
                    <dir-pagination-controls boundary-links="true" on-page-change="pageChangeHandler(newPageNumber)" template-url="/views/dirPagination.tpl.ejs"></dir-pagination-controls>
                </div>
            </div>
        </div>
    </div>

</div>