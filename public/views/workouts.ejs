<br>
<div class="container">
        <div class="col-sm-12">
            <div class="col-xs-4">
                <div class="form-group">
                    <div class="input-group input-group-rounded">
                  <span class="input-group-btn">
                    <button class="btn" type="submit"><span class="fui-search"></span></button>
                  </span>
                        <input ng-model="q" id="search" class="form-control" placeholder="Search">
                    </div>
                </div>
            </div>
        </div>
        <div class="col-sm-12">
            <div class="col-sm-12 col-md-6 col-lg-6 col-xl-4" ng-init="p = this"
                 dir-paginate="workout in WorkoutsWithExercises | filter:q | itemsPerPage: pageSize"
                 current-page="currentPage"
                 style="padding: 10px;">
                <div class="panel panel-success" style="margin:0;padding:0;border: 0">
                    <div class="panel-heading" style="padding: 0;background-color: #16a085;color: white">
                        <i class="fui fui-cross pull-right text-danger" ng-show="workout.private_workout == 1"
                           style="margin-right: 5px" ng-click(delete_workout(workout))></i>

                        <p style="font-size: 16px;margin-top: 5px;margin-bottom: 0;text-align: center;font-weight: bolder">
                                {{workout.workoutName}}</p>

                    </div>
                    <div class="panel-body" style="padding: 0">
                        <table class="table">
                            <thead>
                            <tr>
                                <th>Name</th>
                                <th>Sets</th>
                            </tr>
                            </thead>

                        </table>
                        <div style="overflow-y: scroll;overflow-x:hidden;height: 150px">
                            <table class="table">
                                <tbody>
                                <tr ng-repeat="exercise in workout.exercises">
                                    <th>{{exercise.name}}</th>
                                    <th>{{exercise.sets}}</th>
                                </tr>
                                </tbody>

                            </table>
                        </div>

                    </div>
                </div>

                <div class="col-lg-12" style=";padding: 0">
                    <div class="btn-group" style="width: 100%">
                        <button class="btn btn-info btn-sm" style="width: 50%;border-top-left-radius: 0"
                                ng-click="openWorkout(workout)">
                            <i class="fui-info-circle"></i>
                        </button>
                        <button class="btn btn-info btn-sm" style="width: 50%;border-top-right-radius: 0"
                                ng-click="openAssignWorkout(workout)">
                            <i class="fui-plus-circle"></i> Assign Workout
                        </button>
                    </div>
                </div>
                <div class="col-lg-12"
                     style="padding: 0; color: #3498DB; text-align: center">
                    <div ng-hide=" workout.rating != null ">
                        No Rating
                    </div>
                    <div ng-show="workout.rating > 0">
                        <uib-rating ng-model="workout.rating" max="max" read-only="isReadonly"
                                    on-hover="hoveringOver(value)"
                                    on-leave="overStar = null" aria-labelledby="default-rating"></uib-rating>
                    </div>

                </div>

            </div>
        </div>
        <div class="col-sm-12">
            <div ng-controller="OtherController" class="other-controller">
                <div class="text-center">
                    <dir-pagination-controls boundary-links="true" on-page-change="pageChangeHandler(newPageNumber)"
                                             template-url="/views/dirPagination.tpl.ejs"></dir-pagination-controls>
                </div>
            </div>
        </div>
    </div>


